# === Database Configuration ===
DB_NAME=           # Name of your database
DB_USER=           # Database username
DB_PASSWORD=       # Database user password
DB_HOST=           # Database host (e.g., localhost or service name in Docker)
DB_PORT=           # Database port (default for PostgreSQL is 5432)

# === Security Settings ===
SECRET_KEY=        # Secret key used to sign JWT tokens (use a long random string)
ALGORITHM=         # Algorithm used for JWT (e.g., HS256)
ACCESS_TOKEN_EXPIRE_MINUTES=  # JWT access token expiration time in minutes (e.g., 30)
